<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F7F7"

    tools:layout_editor_absoluteY="81dp">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="79dp"
        android:layout_height="39dp"
        android:text="민영"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="228dp"
        android:layout_height="100dp"
        android:background="@drawable/shape_button"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.04000002">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/c177757"
            android:baselineAligned="false" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/c123457"
            android:layout_marginTop="5dp"
            />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:adjustViewBounds="false"
            android:background="@drawable/c127456"
            android:cropToPadding="false"
            android:layout_marginTop="5dp"
            />
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="87dp"
        android:layout_height="37dp"

        android:text="미선"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="301dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"

        android:layout_alignParentStart="true"
        android:background="@drawable/shape_button"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.06999999">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="@drawable/c177757"
            android:layout_marginTop="5dp"
            />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/c173757"
            android:layout_marginTop="5dp"
            />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:background="@drawable/c777776"
            android:layout_marginTop="5dp"
            />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="90dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/c723456"
            android:layout_marginTop="5dp"
            />


    </LinearLayout>

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="34dp" />


</android.support.constraint.ConstraintLayout>